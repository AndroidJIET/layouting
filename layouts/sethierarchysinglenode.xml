<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/sethierarchysinglenodelayouttextView"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:background="@drawable/roundtextview"
        android:padding="8dp"
        android:layout_marginTop="8dp"
        android:text="administrator"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sethierarchysinglenodelayouttextViewimageview" />

    <ImageButton
        android:id="@+id/sethierarchysinglenodelayouttextViewimagebutton"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/ic_close"
        app:layout_constraintBottom_toTopOf="@+id/sethierarchysinglenodelayouttextView"
        app:layout_constraintEnd_toEndOf="@+id/sethierarchysinglenodelayouttextView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/sethierarchysinglenodelayouttextView"
        app:layout_constraintTop_toTopOf="@+id/sethierarchysinglenodelayouttextView"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/sethierarchysinglenodelayouttextViewimageview"
        android:layout_width="41dp"
        android:layout_height="37dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/ic_arrow"
        app:layout_constraintEnd_toEndOf="@+id/sethierarchysinglenodelayouttextView"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="@+id/sethierarchysinglenodelayouttextView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>